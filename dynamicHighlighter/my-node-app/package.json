{
  "name": "dynamic-highlighter",
  "version": "1.0.0",
  "description": "VS Code extension for dynamic highlighting",
  "main": "out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "vscode": "^1.1.37"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FHobbs8030/dynamicHighlighter.git"
  },
  "keywords": [
    "VSCode",
    "extension",
    "highlighting"
  ],
  "author": "Fred Hobbs",
  "license": "ISC"
}
